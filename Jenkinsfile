pipeline {
    agent any 
        stages{
            stage('checkout'){
                steps{
                        sh 'echo "checkout stage"'
                }
             }
            stage('Building'){
                steps{
                        sh 'echo "Build stage"'
                }
             }
            stage('Testing'){
                steps{
                        sh 'echo "Testing stage"'
                }
            }
        }
    }